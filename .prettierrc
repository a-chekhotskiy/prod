{
    "arrowParens": "always",
    "bracketSpacing": true,
    "endOfLine": "lf",
    "htmlWhitespaceSensitivity": "css",
    "jsxBracketSameLine": true,
    "jsxSingleQuote": false,
    "printWidth": 150,
    "proseWrap": "preserve",
    "quoteProps": "as-needed",
    "semi": true,
    "singleQuote": true,
    "stylelintIntegration": false,
    "tabWidth": 4,
    "trailingComma": "all",
    "useTabs": false,
    "importOrder": ["(^(api|data|locales|modules|shared|store)/(.*)$)|api|data|locales|modules|shared|store", "^[./]"],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
}
